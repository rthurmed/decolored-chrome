{
  "manifest_version": 3,
  
  "name": "Decolored",
  "description": "Apply black and white filter to the page",
  "version": "0.1",

  "background": {
    "service_worker": "background.js"
  },

  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  
  "action": {
    "default_popup": "popup.html"
  }
}